{% extends 'layouts/main.twig' %}

{% block content %}

<div class="container">
  {% for i in 2..10 %}
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <p>
        <a href="/wizard/result/type/{{ type }}/duration/{{ duration }}/envy/{{ envy }}/players/{{ i }}" class="btn btn-large btn-full btn-info">{{ i }}</a>
      </p>
    </div>
  </div>
  {%  endfor %}
</div>
{% endblock %}